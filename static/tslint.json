{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true, "single"
        ],
        "variable-name": [
            true, "allow-leading-underscore"
        ],
        "object-literal-key-quotes": [
            true, "as-needed"
        ]
    },
    "rulesDirectory": []
}
